{
     "name": "Beat the Cheat",
     "description": "A chrome extension to prevent cheating in online tests.",
     "version": "0.1.0",
     "manifest_version": 2,
     "background":{
          "scripts":[
               "./background.js"
          ]
     },
     "options_page":"options.html",
     "browser_action":{
          "default_popup":"popup.html"
     },
     "permissions":[
          "tabs",
          "https://www.google.com/*"
     ]
}